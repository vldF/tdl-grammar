type Test(a, b, c);

function func(a);
function func(a, b);
function func(a, b, c);

invoke on Test {
    return func(a) + func(a, b) + func(a, b, c);
};

function main() {
    var = Test(1, 2, "string"); // ok
    var(); // ok

    badVar = Test(); // unmatching argument
    badVar(); // unresolved
};